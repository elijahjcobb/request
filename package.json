  "keywords": [
	"request",
	"response",
	"promise",
	"bearer",
	"authorization",
	"header",
	"body"
  ],
  "author": "Elijah Cobb <elijah@elijahcobb.com> (https://elijahcobb.com/)",
  "license": "MIT",
  "bugs": {
	"url": "https://github.com/elijahjcobb/request/issues"
  },
  "homepage": "https://github.com/elijahjcobb/request#readme",
  "dependencies": {
	"request": "latest"
  },
  "devDependencies": {
	"@types/node": "latest",
	"typescript": "latest",
	"tslint": "latest",
	"@types/request": "latest"
  },
  "files": [
	"dist"
  ]
}
